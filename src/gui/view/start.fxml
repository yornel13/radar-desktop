<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" prefHeight="585.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <AnchorPane prefHeight="585.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
      <children>
         <Pane prefHeight="585.0" prefWidth="860.0" style="-fx-background-color: #03a9f4;">
            <children>
               <JFXButton fx:id="buttonImport" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" focusTraversable="false" layoutX="760.0" layoutY="25.0" onAction="#importFile" style="-fx-background-color: rgba(0, 0, 0, 0.05);" text="Importar" textFill="WHITE">
                  <font>
                     <Font size="15.0" />
                  </font></JFXButton>
               <JFXButton fx:id="buttonExport" alignment="CENTER" buttonType="RAISED" contentDisplay="CENTER" focusTraversable="false" layoutX="665.0" layoutY="25.0" onAction="#exportFile" style="-fx-background-color: rgba(0, 0, 0, 0.05);" text="Exportar" textFill="WHITE">
                  <font>
                     <Font size="15.0" />
                  </font></JFXButton>
               <JFXButton fx:id="optionAdmin" focusTraversable="false" layoutX="20.0" layoutY="14.0" text="Administradores" textFill="WHITE">
                  <font>
                     <Font size="14.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </JFXButton>
               <JFXButton fx:id="optionEnterprise" focusTraversable="false" layoutX="200.0" layoutY="14.0" text="Empresas" textFill="WHITE">
                  <font>
                     <Font size="14.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </JFXButton>
               <JFXButton fx:id="optionControl" focusTraversable="false" layoutX="340.0" layoutY="14.0" text="Marcadores" textFill="WHITE">
                  <font>
                     <Font size="14.0" />
                  </font>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </JFXButton>
               <Label fx:id="labelEmpresa" layoutX="490.0" layoutY="272.0" text="Empresa" textFill="WHITE" textOverrun="CLIP">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <JFXButton layoutX="695.0" layoutY="355.0" onAction="#onEnter" prefWidth="90.0" style="-fx-background-color: #ffc107;" text="ENTRAR" />
               <ComboBox fx:id="selector" layoutX="490.0" layoutY="305.0" onAction="#onSelectCompany" prefWidth="300.0" style="-fx-background-color: white;" />
            </children>
         </Pane>
      </children>
   </AnchorPane>
</StackPane>
