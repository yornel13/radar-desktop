<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXDialog?>
<?import com.jfoenix.controls.JFXDialogLayout?>
<?import com.jfoenix.controls.JFXDrawer?>
<?import com.jfoenix.controls.JFXListView?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.lynden.gmapsfx.GoogleMapView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" prefHeight="585.0" prefWidth="860.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane fx:id="anchorPane" prefHeight="585.0" prefWidth="860.0">
        <children>
         <JFXButton fx:id="backButton" alignment="BASELINE_LEFT" prefHeight="45.0" prefWidth="275.0" style="-fx-background-color: white;" />
         <JFXTextField fx:id="filterField" alignment="BASELINE_LEFT" focusColor="#0288d1" labelFloat="true" layoutX="68.0" layoutY="1.0" prefHeight="35.0" prefWidth="200.0" promptText="Buscar" unFocusColor="#909090">
            <font>
               <Font size="13.0" />
            </font>
         </JFXTextField>
         <JFXTextField fx:id="drawerFilterField" alignment="BASELINE_LEFT" focusColor="#0288d1" labelFloat="true" layoutX="68.0" layoutY="1.0" prefHeight="35.0" prefWidth="200.0" promptText="Buscar" unFocusColor="#909090" visible="false">
            <font>
               <Font size="13.0" />
            </font>
         </JFXTextField>
         <Pane fx:id="paneDate" layoutY="45.0" prefHeight="35.0" prefWidth="275.0" style="-fx-background-color: white;">
            <children>
               <JFXButton fx:id="searchButton" layoutX="240.0" prefHeight="30.0" prefWidth="30.0" />
               <JFXDatePicker fx:id="fromPicker" layoutX="10.0" prefHeight="30.0" prefWidth="110.0" />
               <JFXDatePicker fx:id="toPicker" layoutX="120.0" prefHeight="30.0" prefWidth="110.0" />
            </children>
         </Pane>
         <JFXListView fx:id="routeListView" layoutX="-2.0" layoutY="80.0" prefHeight="505.0" prefWidth="275.0" />
         <JFXDrawer fx:id="drawer" defaultDrawerSize="300.0" layoutX="-3.0" layoutY="45.0" prefHeight="540.0" prefWidth="275.0" visible="false">
            <children>
               <VBox fx:id="drawerBox" prefHeight="540.0" prefWidth="275.0">
                  <children>
                     <Pane prefHeight="100.0" prefWidth="275.0">
                        <children>
                           <HBox fx:id="headHBox" alignment="BOTTOM_LEFT" prefHeight="70.0" prefWidth="275.0" />
                           <Label layoutY="75.0" prefHeight="20.0" text="   Lista de guardias efectuadas" textFill="#000000a3">
                              <font>
                                 <Font name="Segoe UI Historic" size="12.0" />
                              </font>
                           </Label>
                           <JFXButton fx:id="printReport" focusTraversable="false" layoutX="230.0" layoutY="45.0" prefHeight="35.0" prefWidth="35.0" ripplerFill="#2b3297" />
                        </children>
                     </Pane>
                     <JFXListView fx:id="drawerListView" focusTraversable="false" prefHeight="450.0" prefWidth="275.0" />
                  </children>
               </VBox>
            </children>
         </JFXDrawer>
         <JFXDrawer fx:id="watchDrawer" defaultDrawerSize="300.0" layoutX="-3.0" layoutY="125.0" prefHeight="460.0" prefWidth="275.0" visible="false">
            <children>
               <VBox fx:id="watchDrawerBox" prefHeight="460.0" prefWidth="275.0">
                  <children>
                     <HBox fx:id="watchHeadHBox" prefHeight="40.0" prefWidth="275.0" />
                     <JFXListView fx:id="pointsListView" focusTraversable="false" prefHeight="420.0" prefWidth="275.0" />
                  </children>
               </VBox>
            </children>
         </JFXDrawer>
            <GoogleMapView fx:id="mapView" layoutX="275.0" prefHeight="585.0" prefWidth="585.0" />
        </children>
    </AnchorPane>
    <JFXDialog fx:id="dialog">
        <JFXDialogLayout>
            <heading>
                <Label fx:id="dialogTitle" />
            </heading>
            <body>
                <Label fx:id="dialogContent" />
            </body>
            <actions>
                <JFXButton fx:id="acceptButton" onAction="#onDialogAccept" styleClass="dialog-accept">ACEPTAR
                </JFXButton>
                <JFXButton fx:id="cancelButton" onAction="#onDialogCancel" styleClass="dialog-accept">CANCELAR
                </JFXButton>
            </actions>
        </JFXDialogLayout>
    </JFXDialog>
</StackPane>
